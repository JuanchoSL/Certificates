{
    "name": "juanchosl/certificates",
    "description": "Little tools collection in order to create and manage keys and certificates",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/JuanchoSL/Certificates",
    "keywords": [
        "crypt",
        "cryptology",
        "openssl",
        "KeyPair",
        "Private key",
        "Public key",
        "Certificate",
        "x509",
        "pkcs12",
        "pkcs7",
        "openssh"
    ],
    "authors": [
        {
            "name": "Juan SÃ¡nchez Lecegui",
            "email": "JuanchoSL@hotmail.com",
            "homepage": "https://github.com/JuanchoSL/"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "juanchosl/exceptions": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "phpstan/phpstan": "1.12.*"
    },
    "autoload": {
        "psr-4": {
            "JuanchoSL\\Certificates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuanchoSL\\Certificates\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --configuration phpunit.xml tests",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}